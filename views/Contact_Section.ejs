

<div id="Contact_Section" style="display: none;">
    <div id="cerrorBox"></div>
    <div id="C_S">
    </div>
    <div id="Contact_Us">
        <h2 id="C_U">
            Contact Us<br/>
        </h2>
    </div>

    <div id="Sub_Title">
        <h3 id="S_T">
            Swing by for a cup of coffee, or leave us a message<br/>
        </h3>
    </div>
    <div id="image">
    </div>
    <div id="image_box">
    </div>
    <form id="contact">

    <div id="cFirst_Name" class="nm-label">
      <span id="F_N">
            First Name<br/>
        </span>
    </div>
    <input type="text" id="Fname_text_box" placeholder="Your name.."/>
    <div id="cLast_Name">
      <span id="cL_N">
            Last Name<br/>
        </span>
    </div>
    <input type="text" id="cLname_box" placeholder="Your last name.."/>
    <div id="cE-mail">
      <span id="cE_M">
            E-mail<br/>
        </span>
    </div>
    <input type="text" id="cEmail_box" placeholder="E-mail.."/>

    <div id="Country">
      <span id="C">
            County<br/>
        </span>
    </div>
    <select id="Country_box">
        <option>
            Australia
        </option>
        <option>
            Canada
        </option>
        <option>
            USA
        </option>
        <option>
            India
        </option>
    </select>
    <div id="Subject">
      <span id="S">
            Subject<br/>
        </span>
    </div>
    <textarea id="Subject_box">Write something..</textarea>
    </form>
    <button id="cSubmit" onclick="ContactSection()">Submit</button>
    <button id="cReset">Reset</button>
</div>
